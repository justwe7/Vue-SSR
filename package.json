    "lint:fix": "eslint --ext .js,.vue src server --fix",
    "lint:css": "stylelint \"src/**/*.(vue|scss|css)\"",
    "prepare": "husky install",
    "serve": "npm run build && cross-env NODE_ENV=production node server",
    "gcm": "cz"
